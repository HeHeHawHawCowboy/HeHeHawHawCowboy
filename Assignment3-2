# Input statements to retrieve test score and class rank
testScoreString = input("Enter the test score: ")  # Get test score as string
classRankString = input("Enter the class rank: ")  # Get class rank as string

# Convert inputs to integers
testScore = int(testScoreString)
classRank = int(classRankString)

# Determine acceptance or rejection
if testScore >= 90:
    if classRank >= 25:
        print("Accept")
    else:
        print("Reject")
elif testScore >= 80:
    if classRank >= 50:
        print("Accept")
    else:
        print("Reject")
elif testScore >= 70:
    if classRank >= 75:
        print("Accept")
    else:
        print("Reject")
else:
    print("Reject")
